Host openwrt-rpi
    HostName 192.168.0.10
    User root

Host ubuntu-amd64
    HostName 192.168.0.18
    User root
    IdentityFile ~/.ssh/id_rsa

Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa

Host ubuntu-amd64-gitea
    HostName 192.168.0.18
    Port 10022
    User git
    IdentityFile ~/.ssh/id_rsa

Host openwrt-rpi-gitea
    HostName 192.168.0.10
    Port 10022
    User git
    IdentityFile ~/.ssh/id_rsa